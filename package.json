{
	"name": "generate-js",
	"version": "0.0.0",
	"description": "generate js bundle from typescript",
	"scripts": {
		"generate": "browserify -p tsify -t [ babelify --extensions .ts ] src/main.ts > public/bundle.js"
	},
	"author": "Konrad Janica <konradjanica@gmail.com> (http://github.com/konradjanica)",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"babel-preset-es2015": "^6.13.2",
		"babelify": "^7.0.0",
		"browserify": "^16.5.1",
		"tsify": "^4.0.1",
		"typescript": "^3.8.3"
	}
}
